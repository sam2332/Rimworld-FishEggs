<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <!-- VCE Fishing Fish Eggs -->
  
  <ThingDef>
    <defName>FishEgg_VCEF_RawAnchovy</defName>
    <label>anchovy egg</label>
    <description>An egg containing anchovy spawn. Can be used to seed saltwater sources.</description>
    <thingClass>FishEggs.ThingWithComps_FishEgg</thingClass>
    <category>Item</category>
    <selectable>true</selectable>
    <useHitPoints>true</useHitPoints>
    <altitudeLayer>Item</altitudeLayer>
    <drawerType>MapMeshOnly</drawerType>
    <tickerType>Rare</tickerType>
    <graphicData>
      <texPath>Things/Item/Resource/AnimalProductRaw/EggRound</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
      <color>(0.8, 0.8, 0.9)</color>
    </graphicData>
    <statBases>
      <MaxHitPoints>25</MaxHitPoints>
      <MarketValue>6</MarketValue>
      <Mass>0.1</Mass>
      <Flammability>0.5</Flammability>
      <DeteriorationRate>1</DeteriorationRate>
    </statBases>
    <soundDrop>Standard_Drop</soundDrop>
    <soundPickup>Standard_Pickup</soundPickup>
    <soundInteract>Interact_Sow</soundInteract>
    <thingCategories>
      <li>FishEggs</li>
    </thingCategories>
    <stackLimit>100</stackLimit>
    <drawGUIOverlay>true</drawGUIOverlay>
    <alwaysHaulable>true</alwaysHaulable>
    <pathCost>1</pathCost>
    <comps>
      <li Class="CompProperties_Rottable">
        <daysToRotStart>5</daysToRotStart>
        <rotDestroys>true</rotDestroys>
      </li>
    </comps>
    <modExtensions>
      <li Class="FishEggs.FishEggProperties">
        <linkedFishDefName>VCEF_RawAnchovy</linkedFishDefName>
        <requiredWaterType>SaltWater</requiredWaterType>
      </li>
    </modExtensions>
  </ThingDef>

