<?xml version="1.0" encoding="utf-8"?>
<Patch>

  <!-- Orbital Bulk Goods Trader -->
  <Operation Class="PatchOperationAdd">
    <xpath>/Defs/TraderKindDef[defName="Orbital_BulkGoods"]/stockGenerators</xpath>
    <value>
      <li Class="StockGenerator_Category">
        <categoryDef>FishEggs</categoryDef>
        <thingDefCountRange>3~8</thingDefCountRange>
        <countRange>5~15</countRange>
      </li>
    </value>
  </Operation>

  <!-- Caravan Bulk Goods Trader -->
  <Operation Class="PatchOperationAdd">
    <xpath>/Defs/TraderKindDef[defName="Caravan_Outlander_BulkGoods"]/stockGenerators</xpath>
    <value>
      <li Class="StockGenerator_Category">
        <categoryDef>FishEggs</categoryDef>
        <thingDefCountRange>2~6</thingDefCountRange>
        <countRange>4~12</countRange>
      </li>
    </value>
  </Operation>

  <!-- Orbital Exotic Goods Trader -->
  <Operation Class="PatchOperationAdd">
    <xpath>/Defs/TraderKindDef[defName="Orbital_Exotic"]/stockGenerators</xpath>
    <value>
      <li Class="StockGenerator_Category">
        <categoryDef>FishEggs</categoryDef>
        <thingDefCountRange>1~4</thingDefCountRange>
        <countRange>3~10</countRange>
      </li>
    </value>
  </Operation>

</Patch>
